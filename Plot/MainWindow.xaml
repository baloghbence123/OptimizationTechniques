<Window x:Class="Plot.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Plot"
        mc:Ignorable="d"
        Title="MainWindow" WindowStartupLocation="CenterScreen" Width="500" Height="500" WindowState="Maximized">
    <Grid x:Name="grid">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="2*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button x:Name="bOne" Grid.Column="1" Grid.Row="0" Margin="60" Click="GeneticV">Genetic Algo Visualize</Button>
        <Button x:Name="bTwo" Grid.Column="1" Grid.Row="1" Margin="60" Click="NSGAIIV">NSGA2 visualize</Button>
        <Button x:Name="bThree" Grid.Column="1" Grid.Row="2" Margin="60" Click="SimulatedA" >Simulated annealing</Button>
        <Canvas x:Name="can"/>
        <Label x:Name="textLabel" ></Label>
    </Grid>
</Window>
